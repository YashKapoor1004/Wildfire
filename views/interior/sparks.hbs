        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1" crossorigin="anonymous">
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js" integrity="sha384-ygbV9kiqUc6oa4msXn9868pTtWMgiQaeYH7/t7LECLbyPA2x65Kgf80OJFdroafW" crossorigin="anonymous"></script>
    </head>
    <body>
        <div class="container">
            <div class="sideBar" id="sideBar">
            <img class="leftArrow" src="/interior/dashboard/leftArrow.svg">
            <h1 id = 'title' class="title">Wildfire</h1>
            <h1 id = 'subTitle' class="subTitle">Surveyer's Portal</h1>
            <img id = 'logo' class="logo" src="/interior/fires/Group 2.svg">
                <div class="buttonsDiv">
                    <a href="dashboard.hbs">
                        <div class="dashDiv" id = 'dashDiv'>
                            <img class="lDash" id="lDash" src="/interior/sparks/dashImg.svg">
                            <h1 class="hDash" id = 'hDash'>Dashboard</h1>
                        </div>
                    </a>
                    <a href="fires.hbs">
                        <div class="firesDiv">
                            <img class="lFires" src="/interior/sparks/firesImg.svg">
                            <h1 class="hFires">Fires</h1>
                        </div>
                    </a>
                    <a href="analytics.hbs">
                        <div class="analyticsDiv">
                            <img class="lAnalytics" src="/interior/fires/analytics.svg">
                            <h1 class="hAnalytics">Analytics</h1>
                        </div>
                    </a>
                    <a href="explore.hbs">
                        <div class="exploreDiv">
                            <img class="lExplore" src="/interior/fires/explore.svg">
                            <h1 class="hExplore">Explore</h1>
                        </div>
                    </a>
                    <a href="settings.hbs">
                        <div class="settingsDiv">
                            <img class="lSettings" src="/interior/fires/settings.svg">
                            <h1 class="hSettings">Settings</h1>
                        </div>
                    </a> 
                </div>
                <img class="profPic" src="/interior/fires/jeff.svg">
                <h1 class="name">Jeffrey Haynes</h1>
                <h1 class="role">Marketing Lead</h1>
                <a href="">
                    <div class= 'hover'>
                        <img class="dot" src="/interior/fires/dot.svg" alt="">
                        <img class="hoverr" src="/interior/fires/hoverr.svg" alt="">
                    </div>
                </a>
            </div>
            <div class="topBar">
                <h1 class="firesHead">Fires</h1>
                <a href="">
                    <div class="covidPop">
                        <h1 class="covidPopulationTxt">COVID-19 Population..</h1>
                        <img class="topArrowDown" src="/interior/sparks/downArrow.svg">
                    </div>
                </a>
                <h1 class="hHOme">Home</h1>
                <img class = 'line' src="/interior/sparks/purpleLine.svg" alt="" />
                <h1 class="hSparks">Sparks</h1>
                <div class="lineSparks"></div>
                <h1 class="hData">Data</h1>
                <div class="lineData"></div>
                <h1 class="hPreferences">Preferences</h1>
                <div class="lineSparks"></div>
                
                <a href="">
                <div class="upgradeDiv">
                    <h1 class="upgrade">Upgrade</h1>
                    <img class='upgradeP' src="/interior/fires/bolt.svg" alt="">
                </div>
        
                </a>
                <a href="">  
                <div class="chat">
                    <img class = 'chatP' src="/interior/fires/chat.svg" alt="">
                </div>
                </a>
                <a href=""> 
                <img class = 'bell' src="/interior/fires/bell.svg" alt="">
                <img class="pfp2" src="/interior/fires/jeff.svg" alt="">
                <img class="arroww" src = '/interior/fires/arrow.svg' alt="">
                </a> 
            </div>
            <div class="content">
                <div class="topTitles">
                    <h1 class="number">#</h1>
                    <h1 class="question">QUESTION</h1>
                    <h1 class="type">TYPE</h1>
                    <h1 class="separation">SEPARATION</h1>
                    <h1 class="responses">RESPONSES</h1>
                    <h1 class="options">OPTIONS</h1>
                </div>
                <div class="data">
                    <img class="number1" src="/interior/sparks/number1.svg">
                    <h1 class="voluntarily">Will you voluntarily take 2 doses of the...</h1>
                    <button class="mc" disabled>Multiple Choice</button>
                    <button class="noSep" disabled>No Separation</button>
                    <h1 class="responseNum">260</h1>
                    <img class="optionsImg" src="/interior/sparks/optionsImg.svg">
                </div>
                <a href="">
                    <div class = 'plus'>
                        <img class ='plusimg'src="/interior/fires/plus.svg" alt="">
                    </div>
                 </a>
            </div>
        </div>
        <script src="/js/exterior/index.js"></script>
